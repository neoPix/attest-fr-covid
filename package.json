{
  "name": "attest-fr-covid",
  "version": "1.1.0",
  "description": "Node library and cli for generating french gov' authorizations during lock-down",
  "repository": {
    "type": "git",
    "url": "https://github.com/neoPix/attest-fr-covid"
  },
  "bin": {
    "attest-fr-covid": "dist/bin.js"
  },
  "main": "dist/index.js",
